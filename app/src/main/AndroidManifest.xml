<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.volks.nanomusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DiscoverMusic"
            android:label="@string/discover_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PopularMusic"
            android:label="@string/popular_name"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".PurchaseMusic"
            android:label="@string/purchase_name"/>
        <activity android:name=".OwnedMusic"
            android:label="@string/owned_name"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>